[32m+[m[32mfrom django.contrib import admin[m
[32m+[m[32mfrom django.contrib.auth import get_user_model[m
[32m+[m[32mfrom .models import FitnessUser, MealPlan, Meal, Progress, Plan, Workout, Exercise[m
[32m+[m
[32m+[m[32mUserModel = get_user_model()[m
[32m+[m
[32m+[m
[32m+[m[32m@admin.register(FitnessUser)[m
[32m+[m[32mclass UserModelAdmin(admin.ModelAdmin):[m
[32m+[m[32m    list_display = ['username', 'first_name', 'last_name', 'age', 'gender'][m
[32m+[m[32m    search_fields = ['username', 'first_name', 'last_name', 'age', 'gender', 'user_role', 'user_level', 'user_goal',[m
[32m+[m[32m                     'user_activity'][m
[32m+[m[32m    list_filter = ['username', 'first_name', 'last_name', 'age', 'gender', 'user_role', 'user_level', 'user_goal',[m
[32m+[m[32m                   'user_activity'][m
[32m+[m[32m    ordering = ['username'][m
[32m+[m
[32m+[m
[32m+[m[32m@admin.register(MealPlan)[m
[32m+[m[32mclass MealPlanModeAdmin(admin.ModelAdmin):[m
[32m+[m[32m    search_fields = ['title', 'description', ][m
[32m+[m[32m    list_display = ['title', 'description', ][m
[32m+[m[32m    list_filter = ['title'][m
[32m+[m[32m    filter_horizontal = ['meals'] \[m
[32m+[m
[32m+[m[32m@admin.register(Exercise)[m
[32m+[m[32mclass ExercisesModelAdmin(admin.ModelAdmin):[m
[32m+[m[32m    list_display = ['name'][m
[32m+[m[32m    search_fields = ['name', 'description', 'muscle_group'][m
[32m+[m[32m    list_filter = ['name', 'muscle_group'][m
[32m+[m
[32m+[m[32m    def __str__(self):[m
[32m+[m[32m        return self.name[m
[32m+[m
[32m+[m
[32m+[m[32m@admin.register(Workout)[m
[32m+[m[32mclass WorkoutsModelAdmin(admin.ModelAdmin):[m
[32m+[m[32m    list_display = ['name'][m
[32m+[m[32m    list_filter = ['name', 'created_by', ][m
[32m+[m[32m    search_fields = ['name', 'created_by', 'exercises'][m
[32m+[m[32m    filter_horizontal = ['exercises'][m
[32m+[m
[32m+[m
[32m+[m[32m@admin.register(Plan)[m
[32m+[m[32mclass PlanModelAdmin(admin.ModelAdmin):[m
[32m+[m[32m    list_display = ['name', 'user'][m
[32m+[m[32m    filter_horizontal = ['workouts'][m
[32m+[m[32m    list_select_related = ['user'][m
[32m+[m[32m    readonly_fields = ['end_date'][m
[32m+[m
[32m+[m
[32m+[m[32m@admin.register(Progress)[m
[32m+[m[32mclass ProgressAdminModel(admin.ModelAdmin):[m
[32m+[m[32m    list_display = ['user'][m
[32m+[m[32m    readonly_fields = ['current_week'][m
