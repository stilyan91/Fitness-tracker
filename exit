[32m+[m[32mfrom django import forms[m
[32m+[m[32mfrom django.contrib.auth import models as auth_models, get_user_model[m
[32m+[m[32mfrom django.contrib.auth import forms as auth_forms[m
[32m+[m[32mfrom django.shortcuts import redirect[m
[32m+[m[32mfrom django.utils.translation import gettext_lazy as _[m
[32m+[m
[32m+[m[32mfrom fitnes_tracker.accounts.models import GENDER, UserRole, UserLevel, UserActivity, UserGoals, Meal, Ingredients[m
[32m+[m
[32m+[m[32mUserModel = get_user_model()[m
[32m+[m
[32m+[m
[32m+[m[32mclass RegisterUserForm(auth_forms.UserCreationForm):[m
[32m+[m[32m    class Meta(auth_forms.UserCreationForm.Meta):[m
[32m+[m[32m        model = UserModel[m
[32m+[m[32m        fields = ['username', 'email', 'password1', 'password2'][m
[32m+[m
[32m+[m[32m    def save(self, commit=True):[m
[32m+[m[32m        user = super().save(commit=False)[m
[32m+[m[32m        if commit:[m
[32m+[m[32m            user.save()[m
[32m+[m[32m        return user[m
[32m+[m
[32m+[m
[32m+[m[32mclass UserDetailsForm(forms.ModelForm):[m
[32m+[m[32m    username = forms.CharField(widget=forms.TextInput(attrs={[m
[32m+[m[32m        "class": "form-username-input",[m
[32m+[m[32m        "placeholder": "Change your username",[m
[32m+[m[32m        "help_text": '',[m
[32m+[m[32m    }))[m
[32m+[m[32m    first_name = forms.CharField(widget=forms.TextInput(attrs={[m
[32m+[m[32m        "class": "form-user-first-name",[m
[32m+[m[32m        "placeholder": "Enter your first name",[m
